; .sgconfig file used for testing

[defaults]
SG_NAMESPACE=cachedb
SG_DRIVER_HOST=localhost
SG_DRIVER_PORT=5432
SG_DRIVER_DB_NAME=${SG_NAMESPACE}
SG_DRIVER_USER=clientuser
SG_DRIVER_PWD=supersecure
SG_DRIVER_CONNECTION_STRING=host=${SG_DRIVER_HOST} port=${SG_DRIVER_PORT} dbname=${SG_DRIVER_DB_NAME} user=${SG_DRIVER_USER} password=${SG_DRIVER_PWD}
SG_DRIVER_ADMIN_USER=clientuser
SG_DRIVER_ADMIN_PWD=supersecure
SG_DRIVER_POSTGRES_DB_NAME=postgres
SG_META_SCHEMA=splitgraph_meta

; Look up repositories on the test remote driver.
SG_REPO_LOOKUP=remote_driver

[remote: local]
SG_DRIVER_HOST=${defaults:SG_DRIVER_HOST}
SG_DRIVER_PORT=${defaults:SG_DRIVER_PORT}
SG_DRIVER_DB_NAME=${defaults:SG_DRIVER_DB_NAME}
SG_DRIVER_USER=${defaults:SG_DRIVER_USER}
SG_DRIVER_PWD=${defaults:SG_DRIVER_PWD}
SG_DRIVER_CONNECTION_STRING=host=${SG_DRIVER_HOST} port=${SG_DRIVER_PORT} dbname=${SG_DRIVER_DB_NAME} user=${SG_DRIVER_USER} password=${SG_DRIVER_PWD}
SG_DRIVER_ADMIN_USER=${defaults:SG_DRIVER_ADMIN_USER}
SG_DRIVER_ADMIN_PWD=${defaults:SG_DRIVER_ADMIN_PWD}
SG_DRIVER_POSTGRES_DB_NAME=${defaults:SG_DRIVER_POSTGRES_DB_NAME}
SG_META_SCHEMA=${defaults:SG_META_SCHEMA}

[remote: remote_driver]
SG_DRIVER_HOST=remote_driver
SG_DRIVER_PORT=5431
SG_DRIVER_DB_NAME=${defaults:SG_DRIVER_DB_NAME}
SG_DRIVER_USER=${defaults:SG_DRIVER_USER}
SG_DRIVER_PWD=${defaults:SG_DRIVER_PWD}
SG_DRIVER_CONNECTION_STRING=host=${SG_DRIVER_HOST} port=${SG_DRIVER_PORT} dbname=${SG_DRIVER_DB_NAME} user=${SG_DRIVER_USER} password=${SG_DRIVER_PWD}
SG_DRIVER_ADMIN_USER=${defaults:SG_DRIVER_ADMIN_USER}
SG_DRIVER_ADMIN_PWD=${defaults:SG_DRIVER_ADMIN_PWD}
SG_DRIVER_POSTGRES_DB_NAME=${defaults:SG_DRIVER_POSTGRES_DB_NAME}
SG_META_SCHEMA=${defaults:SG_META_SCHEMA}
